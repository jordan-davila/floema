{
    "name": "boilerplate",
    "version": "1.0.0",
    "scripts": {
        "start": "concurrently --kill-others \"npm run backend:dev\" \"npm run frontend:dev\"",
        "backend:build": "node app.js",
        "backend:dev": "nodemon app.js",
        "frontend:build": "webpack --progress --config webpack.config.build.js",
        "frontend:dev": "cross-env NODE_ENV=dev webpack serve --progress --config webpack.config.development.js"
    },
    "author": "Jordan Davila",
    "license": "ISC",
    "devDependencies": {
        "@prettier/plugin-pug": "^1.16.4",
        "babel-loader": "^8.2.2",
